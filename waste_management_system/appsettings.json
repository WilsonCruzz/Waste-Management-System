{
  "ConnectionStrings": {
    "DefaultConnection": "${DB_DefaultConnection}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Facebook": {
      "AppId": "${FACEBOOK_APP_ID}",
      "AppSecret": "${FACEBOOK_APP_SECRET}"
    }
  },
  "GoogleOAuthClientId": "${GOOGLE_OAUTH_CLIENT_ID}",
  "GoogleOAuthClientKey": "${GOOGLE_OAUTH_CLIENT_KEY}"
}